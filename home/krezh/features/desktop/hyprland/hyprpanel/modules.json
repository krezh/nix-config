{
  "custom/wallpaper": {
    "icon": "󰸉",
    "label": "",
    "tooltip": "Click to change wallpaper",
    "truncationSize": -1,
    "hideOnEmpty": false,
    "actions": {
      "onLeftClick": "swww-random -d ~/wallpapers --fps 60 --step 120 --transition grow --pos mouse -o"
    }
  },
  "custom/github": {
    "icon": "󰊤",
    "label": "{count}",
    "tooltip": "GitHub notifications: {count}",
    "hideOnEmpty": true,
    "interval": 300000,
    "execute": "gh api notifications --jq 'length' 2>/dev/null | xargs -I{} echo '{\"count\": {}}' || echo ''",
    "actions": {
      "onLeftClick": "xdg-open https://github.com/notifications",
      "onRightClick": "gh api notifications -X PUT"
    }
  }
}
